            <div class="form-group">
                <label class="w-12 control-label" style="font-family: 微软雅黑;font-size: 16px">商品编号：</label>
                <div class="w-20 relative ">
                    <input style="border-left-width:0px;border-top-width:0px;border-right-width:0px;border-bottom-color:black" id="product_name" class="form-control" type="text" name="home_title1" maxlength="12" value="" >
                </div>
            </div>
            <div class="form-group ">
                <label class="control-label w-12">商品条形码：</label>
                <div class="w-40">
                    <input id="w_id" class="form-control" type="text" name="w_id"   maxlength="20" value="" >
                </div>
            </div>
            <div class="form-group ">
                <label class="control-label w-12">中文名称：</label>
                <div class="w-40">
                    <input id="w_id" class="form-control" type="text" name="w_id"   maxlength="20" value="" >
                </div>
            </div>
            <div class="form-group ">
                <label class="control-label w-12">英文名称：</label>
                <div class="w-40">
                    <input id="w_id" class="form-control" type="text" name="w_id"   maxlength="20" value="" >
